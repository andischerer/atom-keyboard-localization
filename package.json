{
  "name": "keyboard-localization",
  "main": "./lib/keyboard-localization",
  "version": "1.4.10",
  "description": "Support for various regional keyboard layouts",
  "repository": "https://github.com/andischerer/atom-keyboard-localization.git",
  "license": "MIT",
  "engines": {
    "atom": ">1.0.2"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.1.0",
    "fs-plus": "^2.0.4"
  },
  "keywords": [
    "keyboard",
    "localization",
    "keybinding",
    "locale",
    "compatibility"
  ],
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "value": "unix",
      "level": "error"
    },
    "max_line_length": {
      "level": "ignore"
    }
  }
}
